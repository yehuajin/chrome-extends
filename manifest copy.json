{
    "manifest_version": 2,
    "name": "注入js",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://lpt.liepin.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["bg.js"] 
    },
    "permissions": [
        "notifications"
    ]
}